@using CI.Entity.ViewModels
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
    ViewData["Title"] = "Landing Page";
    Layout = "_HeaderFooter";

}
@model CI.Entity.ViewModels.landing;

  


<partial name="_FilterPartialView" />
@*<h1>Landing Page</h1>*@
 <div class="landingbody flex-grow-1">

        <div class="history" id="history">
            <div class="rounded-pill pilltextcolor myroundedpill" style="height: 27px;"><span>Tree Plantation
                    <img src="~/images/cancel.png" alt="cancel"></i>
                </span>

            </div>

            <div class="rounded-pill pilltextcolor myroundedpill" style="height: 27px;"><span>Tree Plantation
                    <img src="~/images/cancel.png" alt="cancel"></i>
                </span>
            </div>
            <div class="rounded-pill pilltextcolor myroundedpill" style="height: 27px;"><span>Tree Plantation
                    <img src="~/images/cancel.png" alt="cancel"></i>
                </span>
            </div>
            <a class=" clearall pilltextcolor " href="#" style="text-decoration: none;" onclick="deletehistory()" > Clear all</a>
        </div>
        <div style="display: flex; margin-top: 7px;align-items: center;margin-bottom: 14px;">
            <span style="font-family:NotoSans-Light; letter-spacing: NaNpx;">Explore &nbsp</span>
            <span style="font-size:large;font-family: NotoSans-Regular;letter-spacing: NaNpx;"><b>72 missions</b></span>
            <div style="display:flex;align-items: center; margin-left: auto;column-gap: 1vw;">

                <div class="dropdown textcolorforlanding layoutsettingfilter"
                    style="border:solid 2px whitesmoke;margin-right: 2vw;">
                    <button style="justify-content: space-evenly;width: 100%;"
                        class="btn-dropdown btn dropdown-toggle d-flex " type="button" data-bs-toggle="dropdown"
                        aria-expanded="false">

                        <span class="textforfilter profiletext " style="padding-right: 5px;">
                            Sort by
                        </span>
                        <img id="arrow" src="~/images/drop-down.png" alt="dropdown-arrow" class="ic-down">
                    </button>
                    <ul class="dropdown-menu">
                        <li><button class="dropdown-item" type="button">Condition 1</button></li>
                        <li><button class="dropdown-item" type="button">Condition 2</button></li>
                        <li><button class="dropdown-item" type="button">Condition 3</button></li>
                    </ul>
                </div>
                <div class="circle" style="padding-left: 2.5px;">
                    <img src="~/images/grid.png" class="iconsmall" id="grid" onclick="gridview()" alt="grid">
                </div>
                <div class="circle" style="padding-left:6px;padding-bottom:7px">
                    <img src="~/images/list.png" style="height:fit-content" class="iconsmall" id="list"
                     onclick="listview()" alt="grid">
                </div>


            </div>
        </div>
        <!-- mission grid starts from here -->
        <!-- mission grid starts from here -->
         
        

        <div class="row row-cols-1 row-cols-md-3 g-4 mb-4 gridrow">
            <!-- first card -->
            <!-- first card -->
            <!-- first card -->

            @{
                   foreach (var item in Model.Missions)
                    {
                       
                   
            
            
            <div class="col col-lg-4 col-md-6 col-sm-12">
                <div class="card h-100">
                    <div class="cardimage">
                        <img src="~/images/Education-Supplies-for-Every--Pair-of-Shoes-Sold.png" class="card-img-top"
                            alt="Education-Supplies-for-Every--Pair-of-Shoes-Sold">
                        <div class="location">
                            <img src="~/images/pin.png">
                            <span>@item.City.CityName  </span>
                        </div>
                         
                        <button class="likebtn">
                            <img src="~/images/heart.png">
                        </button>
                        <button class="sharebtn">
                            <img src="~/images/user.png">
                        </button>

                        <div class="missiontheme">
                            <span>
                                @item.MissionTheme.Title
                            </span>
                        </div>
                    </div>
                    <div class="card-body">
                        <h5 class="card-title cardtitletext">@item.Title</h5>

                        <p class="card-text carddesctext">@item.Description</p>
                        <div class="rating">
                            <span class="cardratingtext">@item.OrganizationName</span>
                            <div class="starcontainer">
                                <img class="star" src="~/images/selected-star.png">
                                <img class="star" src="~/images/selected-star.png">
                                <img class="star" src="~/images/selected-star.png">
                                <img class="star" src="~/images/star.png">
                                <img class="star" src="~/images/star.png">
                            </div>
                        </div>

                    </div>

                    <!-- vacancy and timeline details starts from here -->
                    <!-- vacancy and timeline details starts from here -->
                    <!-- vacancy and timeline details starts from here -->
                    <div class="cardnumberdetails card-footer">
                        <div class="rounded-pill pilltextcolor cardtimesheetpill deadlinetext">
                               
                                @if (item.MissionType.Equals("Time"))
                                {

                                    <span>From @item.StartDate.Value.ToShortDateString() until @item.EndDate.Value.ToShortDateString()
                                    </span>
                                }else{
                                        <span>@item.GoalMissions[0].GoalObjectiveText
                                        </span>
                                    }

                                

                   
                         </div>
                        <div class="cardnumberdetails2">
                            <div class="cardnumberdetails3 col-6">
                                <div>
                                    <img src="~/images/Already-volunteered.png" class="cardicon"
                                        alt="Already volunteered">
                                </div>
                                <div>
                                    <p class="cardnumeric">250
                                        <span class="cardnumerictext">Already volunteered</span>
                                    </p>

                                </div>


                            </div>

                            <div class="cardnumberdetails3 col-6">
                                <div>
                                    <img src="~/images/deadline.png" class="cardicon" alt="Deadline">
                                </div>
                                <div>
                                    <p class="cardnumeric">09/01/2019
                                        <span class="cardnumerictext">Deadline</span>
                                    </p>

                                </div>


                            </div>

                        </div>
                    </div>

                    <div class="card-footer cardfooter">
                        <button type="button" class="btn btn-outline-warning apply">
                            <div class="applytext">Apply</div>
                            <div>
                                <img class="right-arrow" src="~/images/right-arrow.png">
                            </div>
                        </button>

                    </div>
                </div>
            </div>
            }
            }
